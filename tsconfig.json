{
  "compilerOptions": {
    "rootDir": "./src",
    "target": "ES2019",
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "moduleResolution": "Node",
    "types": [],
    "skipLibCheck": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "./node_modules/*",
    "./node_modules",
    "node_modules"
  ]
}